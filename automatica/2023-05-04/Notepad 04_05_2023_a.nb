 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["g", "TI"], 
         RowBox[{"(", 
           StyleBox["k", "TI"], ")"}], "\[LongEqual]", 
         SuperscriptBox[
          StyleBox["Z", "TI"], "-1"], 
         RowBox[{"(", 
           RowBox[{
             StyleBox["G", "TI"], 
             RowBox[{"[", 
               StyleBox["z", "TI"], "]"}]}], ")"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> "g(k)=\\mathcal{Z}^{-1}\\left(G[z]\\right)", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "990c3357-b393-4065-aefa-14b44bc82425"],
