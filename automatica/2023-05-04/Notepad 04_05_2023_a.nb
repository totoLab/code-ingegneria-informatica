       RowBox[{
         SubscriptBox[
          StyleBox["D", "TI"], "0"], "+", 
         SubscriptBox[
          StyleBox["D", "TI"], "1"], " ", 
         FractionBox[
          StyleBox["z", "TI"], 
          RowBox[{
            StyleBox["z", "TI"], "+", 
            FractionBox["1", "2"]}]], "+", 
         SubscriptBox[
          StyleBox["D", "TI"], "2"], " ", 
