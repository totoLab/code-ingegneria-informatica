 CellLabel->"In[3]:=",ExpressionUUID->"4d799fef-27e5-4ece-b06f-a43d7a120f68"],

Cell[BoxData[
 FractionBox[
  RowBox[{"1", "-", "s"}], 
  RowBox[{"10", "+", 
   RowBox[{"17", " ", "s"}], "+", 
   RowBox[{"8", " ", 
    SuperscriptBox["s", "2"]}], "+", 
   SuperscriptBox["s", "3"]}]]], "Output",
