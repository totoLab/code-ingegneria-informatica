Cell[TextData[{
 "Applico la Formula semplificata di Heaviside per calcolarmi i coefficienti ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["C", "TI"], 
          StyleBox["i", "TI"]], ", ", 
         StyleBox["i", "TI"], "\[LongEqual]", "1", ",", "\[Ellipsis]", ",", 
         "4"}], TraditionalForm], "errors" -> {}, "input" -> 
