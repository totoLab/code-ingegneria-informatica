Cell[BoxData[
 FractionBox[
  RowBox[{"1", "-", "s"}], 
  RowBox[{"10", "+", 
   RowBox[{"17", " ", "s"}], "+", 
   RowBox[{"8", " ", 
    SuperscriptBox["s", "2"]}], "+", 
   SuperscriptBox["s", "3"]}]]], "Output",
 CellChangeTimes->{3.8915765332971287`*^9, 3.891578064350443*^9},
